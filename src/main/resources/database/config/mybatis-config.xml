		<typeAlias type="com.kwan.app.members.MemberDTO" alias="MemberDTO"/>
		<typeAlias type="com.kwan.app.members.MemberFileDTO" alias="MemberFileDTO"/>
		<typeAlias type="com.kwan.app.accounts.AccountDTO" alias="AccountDTO"/>
		
		<typeAlias type="com.kwan.app.trades.TradeDTO" alias="TradeDTO"/>
		<typeAlias type="com.kwan.app.boards.notice.NoticeDTO" alias="NoticeDTO"/>
		<typeAlias type="com.kwan.app.boards.notice.NoticeFileDTO" alias="NoticeFileDTO"/>
		<typeAlias type="com.kwan.app.boards.qna.QnaDTO" alias="QnaDTO"/>
		<typeAlias type="com.kwan.app.boards.qna.QnaFileDTO" alias="QnaFileDTO"/>
		<typeAlias type="com.kwan.app.boards.BoardDTO" alias="BoardDTO"/>
		<typeAlias type="com.kwan.app.boards.BoardFileDTO" alias="BoardFileDTO"/>
		<typeAlias type="com.kwan.app.files.FileDTO" alias="FileDTO"/>	
		<typeAlias type="com.kwan.app.util.Pager" alias="Pager"/>	
		<typeAlias type="java.util.List" alias="List"/>
		<typeAlias type="java.util.Map" alias="Map"/>
		
	</typeAliases>  
  
</configuration>