<!-- database-context에서 연결해야함 -->
<!-- namespace 는 type X, String 타입이라 사용불가 -->
  
<configuration>

	<typeAliases>
		<typeAlias type="com.kwan.app.product.ProductDTO" alias="ProductDTO"/>
